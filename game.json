{
	"width": 1280,
	"height": 720,
	"fps": 15,
	"main": "./script/main.js",
	"assets": {
		"player": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/player.png"
		},
		"shot": {
			"type": "image",
			"width": 16,
			"height": 16,
			"path": "image/shot.png"
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"utils": {
			"type": "script",
			"path": "script/utils.js",
			"global": true
		},
		"cell": {
			"type": "script",
			"path": "script/cell.js",
			"global": true
		},
		"board": {
			"type": "script",
			"path": "script/board.js",
			"global": true
		},
		"cellGenerator": {
			"type": "script",
			"path": "script/cellGenerator.js",
			"global": true
		}
	},
	"moduleMainScripts": {},
	"environment": {
		"sandbox-runtime": "3"
	}
}